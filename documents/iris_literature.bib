 title = {The tick Ixodes ricinus: distribution and climate preferences in the western Palaearctic},
 keywords = {clades;habitat suitability modelling;Ixodes ricinus;normalized difference vegetation index-derived ecoregions},
 pages = {189--197},
 volume = {20},
 number = {2},
 issn = {0269-283X},
 journal = {Medical and Veterinary Entomology},
 doi = {10.1111/j.1365-2915.2006.00622.x},
 file = {Estrada-Peña, VENZAL et al 2006 - The tick Ixodes ricinus:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Estrada-Peña, VENZAL et al 2006 - The tick Ixodes ricinus.pdf:pdf}
