 journal = {Scientific reports},
 doi = {10.1038/s41598-017-05231-1},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5503982},
 file = {Belli, Sarr et al. 2017 - Ticks infected via co-feeding transmission:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Belli, Sarr et al. 2017 - Ticks infected via co-feeding transmission.pdf:pdf}
