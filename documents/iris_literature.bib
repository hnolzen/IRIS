 pages = {15478},
 volume = {10},
 number = {1},
 journal = {Scientific reports},
 doi = {10.1038/s41598-020-71920-z},
 file = {Liebig, Boelke et al 2020 - Tick populations from endemic:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Liebig, Boelke et al 2020 - Tick populations from endemic.pdf:pdf}
