 journal = {Journal of Medical Entomology},
 doi = {10.1093/jme/tjw116},
 file = {Brugger, Boehnke et al 2016 - A Density Map:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Brugger, Boehnke et al 2016 - A Density Map.pdf:pdf}
