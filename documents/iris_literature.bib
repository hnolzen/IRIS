 journal = {Regional Environmental Change},
 doi = {10.1007/s10113-013-0499-2},
 file = {Jacob, Petersen et al 2014 - EURO-CORDEX:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Jacob, Petersen et al 2014 - EURO-CORDEX.pdf:pdf}
