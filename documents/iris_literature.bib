 doi = {10.1111/nyas.13950},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6378404},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/30120891},
 file = {Caminade, McIntyre et al. 2019 - Impact of recent and future:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Caminade, McIntyre et al. 2019 - Impact of recent and future.pdf:pdf}
