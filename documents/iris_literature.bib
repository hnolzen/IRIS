 journal = {Experimental and Applied Acarology},
 doi = {10.1007/s10493-018-0267-6},
 file = {Brugger, Walter et al 2018 - Forecasting next season’s Ixodes ricinus:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Brugger, Walter et al 2018 - Forecasting next season’s Ixodes ricinus.pdf:pdf}
