 journal = {Ticks and Tick-borne Diseases},
 doi = {10.1016/j.ttbdis.2011.03.002},
 file = {Dobson, Taylor et al 2011 - Tick Ixodes ricinus abundance:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Dobson, Taylor et al 2011 - Tick Ixodes ricinus abundance.pdf:pdf}
