 pages = {699--714},
 volume = {52},
 number = {7},
 issn = {1437-1588},
 journal = {Bundesgesundheitsblatt - Gesundheitsforschung - Gesundheitsschutz},
 doi = {10.1007/s00103-009-0874-9},
 file = {Stark, Niedrig et al 2009 - Die Auswirkungen des Klimawandels:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Stark, Niedrig et al 2009 - Die Auswirkungen des Klimawandels.pdf:pdf}
}


