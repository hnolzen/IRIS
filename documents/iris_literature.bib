 issn = {0021-8901},
 journal = {Journal of Applied Ecology},
 doi = {10.1111/j.1365-2664.2011.02004.x},
 file = {Dobson, Randolph 2011 - Modelling the effects of recent:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Dobson, Randolph 2011 - Modelling the effects of recent.pdf:pdf}
