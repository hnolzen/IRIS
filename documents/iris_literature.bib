 journal = {Experimental and Applied Acarology},
 doi = {10.1007/s10493-020-00511-4},
 file = {Vogelgesang, Walter et al 2020 - Long-term monitoring of the seasonal:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Vogelgesang, Walter et al 2020 - Long-term monitoring of the seasonal.pdf:pdf}
