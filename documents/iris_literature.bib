 file = {Randolph 2002 - Predicting the risk of tick-borne:C\:\\Users\\nolzen\\Documents\\Citavi 6\\Projects\\EcoEpi\\Citavi Attachments\\Randolph 2002 - Predicting the risk of tick-borne.pdf:pdf}
}


