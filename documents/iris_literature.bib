 author = {Danielov{\'a}, Vlasta and Daniel, Milan and Schwarzov{\'a}, Lucie and Materna, Jan and Rudenko, Natalia and Golovchenko, Marina and Holubov{\'a}, Jaroslava and Grubhoffer, Libor and Kili{\'a}n, Patrik},
 year = {2010},
 title = {Integration of a Tick-Borne Encephalitis Virus and Borrelia burgdorferi sensu lato into Mountain Ecosystems, Following a Shift in the Altitudinal Limit of Distribution of Their Vector, Ixodes ricinus (Krkono{\v{s}}e Mountains, Czech Republic)},
